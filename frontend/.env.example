# Frontend Environment Variables

# Application Configuration
NEXT_PUBLIC_APP_NAME=QuickGame
NEXT_PUBLIC_APP_VERSION=1.0.0

# Network Configuration
NEXT_PUBLIC_DEFAULT_CHAIN_ID=421614
NEXT_PUBLIC_ENABLE_TESTNETS=true

# Contract Addresses (will be updated after deployment)
NEXT_PUBLIC_CONTRACT_ADDRESS_ARBITRUM_SEPOLIA=0x7296c77Edd04092Fd6a8117c7f797E0680d97fa1
NEXT_PUBLIC_CONTRACT_ADDRESS_LINEA_SEPOLIA=0x7296c77Edd04092Fd6a8117c7f797E0680d97fa1
NEXT_PUBLIC_CONTRACT_ADDRESS_BSC_TESTNET=0x7296c77Edd04092Fd6a8117c7f797E0680d97fa1

# RPC URLs (optional - will use default if not provided)
NEXT_PUBLIC_ARBITRUM_SEPOLIA_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc
NEXT_PUBLIC_LINEA_SEPOLIA_RPC_URL=https://rpc.sepolia.linea.build
NEXT_PUBLIC_BSC_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545

# WalletConnect Project ID (required for WalletConnect)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Analytics and Monitoring (optional)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=
NEXT_PUBLIC_SENTRY_DSN=

# Feature Flags
NEXT_PUBLIC_ENABLE_GAME_HISTORY=true
NEXT_PUBLIC_ENABLE_LEADERBOARD=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true

# Development Settings
NEXT_PUBLIC_DEBUG_MODE=false
NEXT_PUBLIC_SHOW_TESTNET_WARNING=true
